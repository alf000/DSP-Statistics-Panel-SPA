#!usr/bin/expect -f"
set email [lindex $argv 0]
set passwd [lindex $argv 1]
spawn heroku login -i
expect -exact " ›   Warning: Our terms of service have changed: "
expect -exact " ›   https://dashboard.heroku.com/terms-of-service"
expect -exact "heroku: Enter your login credentials"
expect -exact "Email:"
send "$email\r"
expect -exact "Password:"
send "$passwd"
send "\r"
expect
send "\r"
expect
puts "Done"
